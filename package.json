    "dist/index.d.ts"
  ],
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/connect": "^3.4.38",
    "@types/dashify": "^1.0.3",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.2",
    "connect": "^3.7.0",
    "dashify": "^2.0.0",
    "eslint": "^9.3.0",
    "globals": "^15.3.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.16"
  }
